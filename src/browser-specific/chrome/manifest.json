{
  "manifest_version": 3,
  "name": "2FAuth",
  "description": "2FAuth extension",
  "permissions": [
    "alarms",
    "clipboardWrite",
    "idle",
    "storage",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "version": "2023.5.0",
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "assets/icons/icon16.png",
    "32": "assets/icons/icon32.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  },
  "background": {
    "service_worker": "service-worker.js"
  }
}
